<%- include('partials/header') %>

<link rel="stylesheet" href="/stylesheets/createBlog.css" />
<header>
  <%- include('partials/nav')%>
  <div class="main">
    <form action="/createBlog" method="post">
      <h1>Create New Blogs</h1>
      <input type="text" placeholder="CreateBy" name="createBy" required />
      <input type="text" placeholder="image URL" name="img" required />
      <input type="text" name="title" placeholder="Title" required />
      <input type="text" name="description" placeholder="Description" />
      <button>Post</button>
      <p>Want to go to homepage ? <a href="/homepage">Homepage</a></p>
    </form>
  </div>
</header>
<%- include('partials/footer') %>
